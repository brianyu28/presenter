{
  "name": "presenter",
  "version": "0.6.5",
  "description": "A JavaScript presentation library",
  "main": "dist/presenter.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/presenter.mjs",
      "require": "./dist/presenter.js"
    },
    "./export": {
      "types": "./dist/export/index.d.ts",
      "import": "./dist/export.mjs",
      "require": "./dist/export.js"
    }
  },
  "files": [
    "./dist/"
  ],
  "scripts": {
    "build": "vite build && vite build --config vite.config.export.ts",
    "watch": "vite build --watch",
    "prettier": "prettier . --write --ignore-unknown",
    "prepare": "husky",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianyu28/presenter.git"
  },
  "author": "Brian Yu <brian@brianyu.me>",
  "bugs": {
    "url": "https://github.com/brianyu28/presenter/issues"
  },
  "homepage": "https://github.com/brianyu28/presenter#readme",
  "license": "GPL-3.0-or-later",
  "sideEffects": false,
  "dependencies": {
    "d3-ease": "^3.0.1",
    "flubber": "^0.4.2",
    "pptxgenjs": "^4.0.1",
    "skia-canvas": "^3.0.8",
    "svg-path-properties": "^2.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/d3-ease": "^3.0.2",
    "@types/flubber": "^0.4.0",
    "@types/jest": "^30.0.0",
    "ajv": "^8.18.0",
    "eslint": "^9.35.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "lint-staged": {
    "**/*": [
      "prettier --ignore-unknown",
      "eslint",
      "jest --findRelatedTests --passWithNoTests"
    ]
  }
}
